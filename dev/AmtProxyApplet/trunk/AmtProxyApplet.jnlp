<?xml version="1.0" encoding="UTF-8"?>
<jnlp spec="1.0+" href="AmtProxyApplet.jnlp" codebase="">
    <information>
        <title>AMT Proxy Server Applet</title>
        <vendor>Greg Bumgardner</vendor>
        <homepage href="http://sites.google.com/site/amtproxy" />
        <description>RTSP Server applet for multicast streaming over AMT</description>
    </information>
    <security>
      <all-permissions/>
    </security>
    <resources>
        <!-- Application Resources -->
        <j2se version="1.6+" href="http://java.sun.com/products/autodl/j2se" />
        <jar href="AmtProxyApplet.jar" version="@APPLET_VERSION@" main="true"/>
        <jar href="jain-sdp-1.0.100.jar"/>
    </resources>
    <applet-desc 
         name="AMT Proxy Server Applet"
         main-class="AmtProxyApplet"
         width="1"
         height="1">
    </applet-desc>
</jnlp>         
