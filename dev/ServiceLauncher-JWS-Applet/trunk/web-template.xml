<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Java Web Start Service Launcher Applet</display-name>
  <servlet>
    <servlet-name>JnlpDownloadServlet</servlet-name>
    <servlet-class>jnlp.sample.servlet.JnlpDownloadServlet</servlet-class>
    <init-param>
      <param-name>logLevel</param-name>
      <param-value>
            DEBUG
            </param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>JnlpDownloadServlet</servlet-name>
    <url-pattern>*.jnlp</url-pattern>
    <url-pattern>*.jar</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>deployLauncher</servlet-name>
    <jsp-file>/deployLauncher.jsp</jsp-file>
    <init-param>
      <param-name>LastModified</param-name>
      <param-value>@LAST_MODIFIED@</param-value>
    </init-param>
    <init-param>
      <param-name>JnlpName</param-name>
      <param-value>@JNLP_NAME@</param-value>
    </init-param>
    <init-param>
      <param-name>MainClass</param-name>
      <param-value>@MAIN_CLASS@</param-value>
    </init-param>
    <init-param>
      <param-name>JarName</param-name>
      <param-value>@JAR_NAME@</param-value>
    </init-param>
    <init-param>
      <param-name>CacheVersion</param-name>
      <param-value>@CACHE_VERSION@</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>deployLauncher</servlet-name>
    <url-pattern>/deployLauncher.js</url-pattern>
  </servlet-mapping>
</web-app>