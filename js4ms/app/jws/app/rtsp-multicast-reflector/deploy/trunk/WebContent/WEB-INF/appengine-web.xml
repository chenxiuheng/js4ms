<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>js4ms-rtsp-reflector</application>
  <version>1</version>

  <threadsafe>true</threadsafe>

  <!-- Configure java.util.logging -->
  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/gae-logging.properties"/>
  </system-properties>

  <static-files>
      <exclude path="*.jar" />
      <exclude path="*.jnlp" />
  </static-files>

</appengine-web-app>
