    <mailingList>
      <name>Commits</name>
      <archive>https://groups.google.com/d/forum/js4ms-commits</archive>
      <subscribe>https://groups.google.com/d/forum/js4ms-commits/subscribe</subscribe>
      <unsubscribe>js4ms-commits+unsubscribe@googlegroups.com</unsubscribe>
