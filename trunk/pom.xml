    <mailingList>
      <name>Community Forum</name>
      <archive>https://groups.google.com/d/forum/js4ms-community-forum</archive>
      <subscribe>https://groups.google.com/d/forum/js4ms-community-forum/subscribe</subscribe>
      <unsubscribe>js4ms-community-forum+unsubscribe@googlegroups.com</unsubscribe>
    </mailingList>
    <mailingList>
      <name>Developer Forum</name>
      <archive>https://groups.google.com/d/forum/js4ms-developer-forum</archive>
      <subscribe>https://groups.google.com/d/forum/js4ms-developer-forum/subscribe</subscribe>
